import java.util.Arrays;
class Main {
  public static void main(String[] args) {
    int num1 []=new int [4];
    int num2 []=new int [4];

    for (int i=0; i<num1.length; i++){
      num1 [i]= i;
      num2 [i]= i-1;
    }
    if (Arrays.equals (num1, num2)){
      System.out.println ("Son iguales");
    } else {
      System.out.println ("No son iguales");
    }
   }
}
